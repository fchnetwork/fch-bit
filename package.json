{
  "name": "nest-typescript-starter",
  "version": "1.0.0",
  "description": "Nest TypeScript starter repository",
  "license": "MIT",
  "scripts": {
    "start": "nodemon index.js",
    "prestart:prod": "tsc",
    "start:prod": "node dist/main.js",
    "jest": "jest --watch",
    "watch": "tsnodemon -x 'ts-node src/main.ts'"
  },
  "dependencies": {
    "@nestjs/common": "^4.3.0",
    "@nestjs/core": "^4.3.0",
    "@nestjs/microservices": "^4.3.0",
    "@nestjs/testing": "^4.3.0",
    "@nestjs/websockets": "^4.3.0",
    "dotenv": "^5.0.1",
    "mongoose": "^5.0.1",
    "redis": "^2.7.1",
    "reflect-metadata": "^0.1.10",
    "rxjs": "^5.4.3",
    "tsnodemon": "^1.1.2",
    "typescript": "^2.4.2",
    "web3": "^1.0.0-beta.34"
  },
  "devDependencies": {
    "@types/jest": "^22.2.3",
    "@types/mongoose": "^4.7.23",
    "@types/node": "^7.0.41",
    "jest": "^22.4.3",
    "ts-jest": "^22.4.5",
    "ts-node": "^3.3.0"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
